generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

model Transaction {
    id              Int      @id @default(autoincrement())
    amountInCents   Int
    summary         String?
    transactionDate DateTime
    merchant        String?
    createdAt       DateTime @default(now())
    updatedAt       DateTime @updatedAt
}
